{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "initialize.result",
  "type": "object",
  "required": ["protocolVersion", "agentInfo", "agentCapabilities", "authMethods"],
  "properties": {
    "protocolVersion": {"type": "integer"},
    "agentInfo": {
      "type": "object",
      "required": ["name", "version"],
      "properties": {
        "name": {"type": "string"},
        "version": {"type": "string"}
      }
    },
    "agentCapabilities": {
      "type": "object",
      "required": ["loadSession", "promptCapabilities", "mcpCapabilities", "sessionCapabilities"],
      "properties": {
        "loadSession": {"type": "boolean"},
        "promptCapabilities": {"type": "object"},
        "mcpCapabilities": {"type": "object"},
        "sessionCapabilities": {"type": "object"}
      }
    },
    "authMethods": {
      "type": "array"
    }
  }
}
